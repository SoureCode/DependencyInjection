{
  "name": "@sourecode/sourecode",
  "version": "0.0.1-DEV",
  "description": "Simply create applications with this framework",
  "keywords": [
    "dependency",
    "injection",
    "dependency injection",
    "application",
    "property",
    "property access",
    "repository",
    "container",
    "service",
    "decorator"
  ],
  "author": "chapterjason <jason.schilling@sourecode.de>",
  "homepage": "https://sourecode.de/",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.{js,d.ts,map}",
    "package.json",
    "README.md",
    "LICENSE.md",
    "yarn.lock"
  ],
  "scripts": {
    "prepack": "yarn run clean && yarn run build && yarn run test",
    "clean": "rm -rf dist && rm -rf node_modules && yarn",
    "test": "jest",
    "build": "tsc",
    "watch": "tsc --watch",
    "debug": "node $NODE_DEBUG_OPTION ./dist/index.js",
    "start": "node ./dist/index.js"
  },
  "dependencies": {
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.12.2",
    "jest": "^24.5.0",
    "ts-jest": "^24.0.1",
    "typescript": "^3.4.1"
  }
}
